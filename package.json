{
  "name": "sign-language-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --configuration=production --base-href=/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/sign-language-playground/stats.json",
    "pwa:assets": "pwa-asset-generator src/assets/icons/dark-logo.svg src/assets/icons/generated/ -i src/index.html -m src/manifest.webmanifest --background white --favicon --mstile && pwa-asset-generator src/assets/icons/light-logo.svg --dark-mode src/assets/icons/generated/ -i src/index.html --background black --splash-only",
    "i18n:find": "transloco-keys-manager find",
    "test": "ng test --code-coverage",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "e2e": "ng e2e"
  },
  "engines": {
    "node": "~0.12"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.4",
    "@angular/cdk": "~12.2.4",
    "@angular/common": "~12.2.4",
    "@angular/compiler": "~12.2.4",
    "@angular/core": "~12.2.4",
    "@angular/forms": "~12.2.4",
    "@angular/material": "~12.2.4",
    "@angular/material-experimental": "^12.2.4",
    "@angular/platform-browser": "~12.2.4",
    "@angular/platform-browser-dynamic": "~12.2.4",
    "@angular/router": "~12.2.4",
    "@angular/service-worker": "~12.2.4",
    "@google/model-viewer": "^1.8.0",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/holistic": "^0.4.1628005088",
    "@ngneat/transloco": "^2.20.1",
    "@ngneat/transloco-messageformat": "^1.3.1",
    "@ngxs/store": "^3.7.2",
    "@sutton-signwriting/font-ttf": "^1.2.0",
    "@sutton-signwriting/sgnw-components": "0.10.0",
    "@tensorflow/tfjs": "~3.8.0",
    "@tensorflow/tfjs-backend-webgl": "~3.8.0",
    "@tensorflow/tfjs-converter": "~3.8.0",
    "@tensorflow/tfjs-core": "~3.8.0",
    "@tensorflow/tfjs-layers": "~3.8.0",
    "cld3-asm": "^3.1.1",
    "comlink": "^4.3.1",
    "flag-icon-css": "^3.5.0",
    "material-components-web": "^12.0.0",
    "ngx-flag-icon-css": "^1.0.1",
    "pose-viewer": "^0.2.3",
    "rxjs": "~6.6.3",
    "seedrandom": "^2.4.3",
    "stats.js": "^0.17.0",
    "three": "0.125.2",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1202.4",
    "@angular-devkit/build-angular": "^12.2.4",
    "@angular/cli": "~12.2.4",
    "@angular/compiler-cli": "~12.2.4",
    "@ngneat/transloco-keys-manager": "^2.7.4",
    "@types/dom-mediacapture-record": "^1.0.10",
    "@types/jasmine": "~3.9.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/three": "^0.129.1",
    "@types/webgl2": "0.0.6",
    "codelyzer": "^6.0.0",
    "fuzzy": "^0.1.3",
    "inquirer": "^7.0.0",
    "inquirer-autocomplete-prompt": "^1.1.0",
    "jasmine-core": "~3.9.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "open": "^8.0.9",
    "protractor": "~7.0.0",
    "pwa-asset-generator": "^4.3.6",
    "sitemap": "^7.0.0",
    "ts-node": "~10.2.1",
    "tslint": "~6.1.3",
    "typescript": "~4.3.5",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
