{
  "name": "sign-translate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "start": "ng serve --host 0.0.0.0",
    "build": "ng build --configuration=production --base-href=/",
    "build:stats": "ng build --stats-json",
    "analyze": "webpack-bundle-analyzer dist/sign-translate/stats.json",
    "pwa:assets": "pwa-asset-generator src/assets/icons/dark-logo.svg src/assets/icons/generated/ -i src/index.html -m src/manifest.webmanifest --background white --favicon --mstile && pwa-asset-generator src/assets/icons/light-logo.svg --dark-mode src/assets/icons/generated/ -i src/index.html --background black --splash-only",
    "i18n:find": "transloco-keys-manager find",
    "test": "ng test --code-coverage",
    "test:chrome": "ng test --browsers=Chrome --code-coverage",
    "test:ci": "ng test --browsers=ChromeHeadless,FirefoxHeadless --watch=false --code-coverage",
    "deploy": "npm run build && node tools/sitemap-generator.js && firebase deploy --only=hosting",
    "prepare:husky": "cp -r ./node_modules/@sign/configuration/.husky/ .husky/",
    "prepare": "husky install && npm run prepare:husky"
  },
  "engines": {
    "node": "16",
    "npm": "8"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.8",
    "@angular/cdk": "~13.3.7",
    "@angular/common": "~13.3.8",
    "@angular/compiler": "~13.3.6",
    "@angular/core": "~13.3.8",
    "@angular/forms": "~13.3.8",
    "@angular/material": "~13.3.7",
    "@angular/material-experimental": "~13.3.7",
    "@angular/platform-browser": "~13.3.8",
    "@angular/platform-browser-dynamic": "~13.3.8",
    "@angular/router": "~13.3.8",
    "@angular/service-worker": "~13.3.8",
    "@google/model-viewer": "^1.12.0",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/holistic": "^0.5.1635989137",
    "@ngneat/transloco": "^4.0.0",
    "@ngxs/store": "^3.7.3",
    "@sign/i18n": "git://github.com/sign/i18n.git",
    "@sutton-signwriting/font-ttf": "^1.3.1",
    "@sutton-signwriting/sgnw-components": "0.10.0",
    "@tensorflow/tfjs": "3.18.0",
    "@tensorflow/tfjs-backend-webgl": "3.18.0",
    "@tensorflow/tfjs-converter": "3.18.0",
    "@tensorflow/tfjs-core": "3.18.0",
    "@tensorflow/tfjs-layers": "3.18.0",
    "cld3-asm": "^3.1.1",
    "comlink": "^4.3.1",
    "flag-icon-css": "^4.1.7",
    "material-components-web": "^14.0.0",
    "ngx-google-analytics": "^13.0.1",
    "pose-viewer": "^0.5.0",
    "rxjs": "~7.5.5",
    "stats.js": "^0.17.0",
    "three": "^0.141.0",
    "tslib": "^2.3.1",
    "web-vitals": "^2.1.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1303.2",
    "@angular-devkit/build-angular": "^13.2.1",
    "@angular-eslint/builder": "^13.2.1",
    "@angular-eslint/eslint-plugin": "^13.2.1",
    "@angular-eslint/eslint-plugin-template": "^13.2.1",
    "@angular-eslint/schematics": "^13.2.1",
    "@angular-eslint/template-parser": "^13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.5",
    "@ngneat/transloco-keys-manager": "^3.2.0",
    "@sign/configuration": "git://github.com/sign/.github.git",
    "@types/dom-speech-recognition": "^0.0.1",
    "@types/jasmine": "~4.0.2",
    "@types/jasminewd2": "~2.0.3",
    "@types/offscreencanvas": "^2019.6.4",
    "@types/three": "^0.141.0",
    "@types/webgl2": "0.0.6",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "deepmerge": "^4.2.2",
    "eslint": "^8.5.0",
    "fuzzy": "^0.1.3",
    "husky": "^8.0.1",
    "inquirer": "^8.2.0",
    "inquirer-autocomplete-prompt": "^2.0.0",
    "jasmine-axe": "^1.1.0",
    "jasmine-core": "~4.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "^6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "^2.0.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-spec-reporter": "^0.0.34",
    "lint-staged": "^13.0.0",
    "open": "^8.0.9",
    "protractor": "~7.0.0",
    "pwa-asset-generator": "^6.0.5",
    "sitemap": "^7.0.0",
    "ts-node": "~10.8.0",
    "typescript": "~4.6.3",
    "webpack-bundle-analyzer": "^4.4.2"
  }
}
